{
    "env": {
        "browser": true,
        "es6": true,
        "amd": true
    },
    "globals": {
        "module": false,
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "2017",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 2, { "MemberExpression": 0 }],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"]
    }
}